#include <stdio.h>
int main()
{    
int number;    
printf("Enter the number: ");    
scanf("%d", & number);
// true if number is perfectly divisible by 2    
if (number % 2 == 0) {        
printf("The given number is EVEN");    
}    
else {        
printf("The given number is ODD");    
}    
return 0;
}
